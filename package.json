{
	"name": "are-you-free-web",
	"type": "module",
	"version": "0.0.1",
	"private": true,
	"packageManager": "pnpm@10.6.3",
	"scripts": {
		"dev": "vite dev --host --port 5173",
		"build": "vite build",
		"preview": "vite preview",
		"start": "vite preview --port 5173",
		"prepare": "husky install",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "eslint . --fix",
		"lint": "eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"codegen": "graphql-codegen"
	},
	"dependencies": {
		"@auth/core": "^0.38.0",
		"@auth/sveltekit": "^1.8.0",
		"@sveltejs/adapter-node": "^5.2.12",
		"@types/node": "^22.13.5",
		"date-fns": "^4.1.0",
		"graphql-request": "^7.1.2",
		"sveltekit:1.7.4": "link:@auth/sveltekit:1.7.4",
		"sveltekit:latest": "link:@auth/sveltekit:latest"
	},
	"devDependencies": {
		"@antfu/eslint-config": "^4.3.0",
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@graphql-codegen/cli": "^5.0.5",
		"@graphql-codegen/typescript-graphql-request": "^6.2.0",
		"@graphql-codegen/typescript-resolvers": "^4.4.4",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.17.2",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/vite": "^4.0.8",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.4",
		"eslint": "^9.21.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-format": "^1.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.14.0",
		"husky": "^9.1.7",
		"jsdom": "^25.0.1",
		"lint-staged": "^15.4.3",
		"lucide-svelte": "^0.475.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"stylelint": "^16.15.0",
		"stylelint-config-html": "^1.1.0",
		"stylelint-config-recess-order": "^6.0.0",
		"stylelint-config-standard": "^37.0.0",
		"stylelint-declaration-block-no-ignored-properties": "^2.8.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-eslint-parser": "^0.43.0",
		"svelte-preprocess": "^6.0.3",
		"tailwindcss": "^4.0.8",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.0.0",
		"vitest": "^3.0.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild",
			"svelte-preprocess"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"**/*.{js,ts,jsx,json,svelte}": [
			"sh -c 'if [ \"$1\" != \"src/lib/graphs/generated/types.ts\" ]; then eslint --fix \"$1\"; fi' --"
		]
	}
}
